{
  "name": "resource-upload-desktop",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "main": "webpack --config webpack.main.js",
    "renderer": "webpack serve --config webpack.renderer.js",
    "serve": "yarn main && yarn renderer",
    "start": "electron ./dist/electron.js",
    "dev": "node runner/dev-runner.js",
    "build": "node runner/build.js && electron-builder"
  },
  "build":{
    "productName": "成果模型查看器",
    "appId": "com.example.yourapp",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "static/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "electron": "^11.2.3",
    "html-webpack-plugin": "^5.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}